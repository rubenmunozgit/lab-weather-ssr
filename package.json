{
  "name": "lab-weather-ssr",
  "version": "1.0.0",
  "description": "SSR for weather app using react",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development --watch",
    "start": "NODE_ENV=production npm run build && node ./build/server.js",
    "start:dev": "concurrently \"npm run build:dev\" \"nodemon ./build/server.js\" ",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rubenmunozgit/lab-weather-ssr.git"
  },
  "author": "Ruben Mu√±oz Palomino",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rubenmunozgit/lab-weather-ssr/issues"
  },
  "homepage": "https://github.com/rubenmunozgit/lab-weather-ssr#readme",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "dependencies": {
    "axios": "^0.19.2",
    "express": "^4.17.1",
    "express-handlebars": "^5.0.0",
    "react": "^16.13.1",
    "react-bootstrap": "^1.2.2",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^1.7.2"
